{
  "manifest_version": 2,
  "name": "Give up Facebook",
  "description": "This extension will slowly help you forget the Facebook",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html",
    "default_title" : "Give up FB"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage", "webRequest", "webRequestBlocking","http://*/*", "https://*/*"
  ]
}